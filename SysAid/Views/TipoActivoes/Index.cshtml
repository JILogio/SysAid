@model IEnumerable<SysAid.TipoActivo>

@{
    ViewBag.Title = "Index";
}

<h2>Tipo de activo</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tipo
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tipo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idTipoActivo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idTipoActivo })
        </td>
    </tr>
}

</table>
